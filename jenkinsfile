    node {
        def dionnesimage

        stage ('build image'){
            checkout scm
            dionnesimage = docker.build("dionnebyrne/cw2")
        }

        stage ('test container'){
        
           dionnesimage.inside{echo 'container is running..'}
            
        }

        stage ('push image'){
            dionnesimage.push("currentbuild")

        }
    }
    

